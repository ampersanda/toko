(ns toko.jigsaw.scrolls
  (:require ["package:flutter/material.dart" :as m]
            [cljd.flutter.alpha :as f]))

(defn default-view [& {:keys [slivers]}]
  (f/widget
    (m/CustomScrollView
      :physics (m/BouncingScrollPhysics
                 :parent (m/AlwaysScrollableScrollPhysics))
      :slivers slivers)))
